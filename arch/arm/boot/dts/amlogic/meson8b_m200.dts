/dts-v1/;
#include "meson8b.dtsi"

/ {
	model = "Meson M8b S805";
	compatible = "amlogic,meson8b";

	aliases {
		serial0 = &uart_AO;
	};

	memory {
		reg = <0x00000000 0x40000000>;
		linux,usable-memory = <0x00200000 0x3fe00000>;
	};
};

&uart_AO {
	status = "okay";
	pinctrl-0 = <&uart_ao_a_pins>;
	pinctrl-names = "default";
};
